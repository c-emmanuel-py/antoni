{
  "name": "grupo-antoni-landing",
  "version": "1.0.0",
  "description": "Landing page for Grupo Antoni - Architecture and Construction Company",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint js/ --ext .js",
    "lint:fix": "eslint js/ --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "optimize:images": "imagemin assets/images/**/*.{jpg,png,svg} --out-dir=assets/images/optimized",
    "serve": "http-server dist -p 8080",
    "deploy": "npm run build && npm run serve"
  },
  "keywords": [
    "architecture",
    "construction",
    "landing-page",
    "responsive",
    "modern"
  ],
  "author": "Grupo Antoni",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "clean-css-cli": "^5.6.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.55.0",
    "http-server": "^14.1.1",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "sharp": "^0.34.4",
    "terser": "^5.44.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "swiper": "^11.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
