{
  "name": "grupo-antoni-landing",
  "version": "1.0.0",
  "description": "Landing page for Grupo Antoni - Architecture and Construction Company",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint js/ --ext .js",
    "lint:fix": "eslint js/ --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "vite build --mode analyze",
    "optimize:images": "imagemin assets/images/**/*.{jpg,png,svg} --out-dir=assets/images/optimized",
    "serve": "http-server dist -p 8080",
    "deploy": "npm run build && npm run serve",
    "prepare": "husky install",
    "quality": "npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format"
  },
  "keywords": [
    "architecture",
    "construction",
    "landing-page",
    "responsive",
    "modern"
  ],
  "author": "Grupo Antoni",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.16",
    "clean-css-cli": "^5.6.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.4",
    "sharp-cli": "^5.2.0",
    "terser": "^5.44.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "swiper": "^11.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
